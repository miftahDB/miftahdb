{
  "name": "miftahdb",
  "description": "Fast and lightweight key-value database library for Node.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./bun": {
      "types": "./dist/bun.d.ts",
      "import": "./dist/bun.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "semantic-release": "semantic-release",
    "test-bun": "bun test ./tests/bun.ts",
    "test-node": "node --import tsx --test node.test.ts",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "database",
    "key-value",
    "SQLite",
    "fast",
    "lightweight",
    "Node.js",
    "miftahdb",
    "kv-store",
    "data storage",
    "kvsqlite"
  ],
  "bugs": {
    "url": "https://github.com/miftahdb/miftahdb/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miftahDB/miftahdb"
  },
  "homepage": "https://miftahdb.sqlite3.online/docs/intro",
  "author": "vwh",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^10.1.7",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/bun": "latest",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^22.5.1",
    "semantic-release": "^24.1.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "better-sqlite3": "^11.2.1",
    "msgpack-lite": "^0.1.26"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
