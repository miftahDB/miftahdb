{
  "name": "miftahdb",
  "version": "0.0.0-development",
  "description": "Fast and lightweight key-value database library for Node.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./bun": {
      "types": "./dist/bun.d.ts",
      "import": "./dist/bun.js",
      "default": "./dist/bun.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "semantic-release": "semantic-release",
    "test-bun": "bun test ./tests/bun.test.ts",
    "test-node": "node --import tsx --test ./tests/node.test.ts",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "database",
    "key-value",
    "SQLite",
    "fast",
    "lightweight",
    "Node.js",
    "miftahdb",
    "kv-store",
    "data storage",
    "kvsqlite"
  ],
  "bugs": {
    "url": "https://github.com/miftahdb/miftahdb/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miftahDB/miftahdb"
  },
  "homepage": "https://miftahdb.sqlite3.online/docs/intro",
  "author": "vwh",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "latest",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^22.10.2",
    "semantic-release": "^24.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "msgpack-lite": "^0.1.26"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
